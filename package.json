{
  "name": "proyecto-arima-backend",
  "version": "0.0.1",
  "description": "Backend for ADAPTAR-IA by Proyecto Arima",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "build": "rimraf dist && tsup",
    "postbuild": "cpy './html_templates/' './dist/'",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --config .prettierrc --write .",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "test:cov": "vitest run --coverage",
    "release": "semantic-release",
    "prepare": "husky",
    "seed_db": "node src/common/utils/seed_db.js"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "@aws-sdk/client-s3": "^3.627.0",
    "@aws-sdk/credential-providers": "^3.624.0",
    "@aws-sdk/s3-request-presigner": "^3.627.0",
    "@types/mongoose": "^5.11.97",
    "aws-sdk": "^2.1671.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cpy-cli": "^4.1.0",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.14",
    "openai": "^4.61.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pino-http": "^10.0.0",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mongodb-memory-server": "^9.4.1",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "semantic-release": "^24.0.0",
    "supertest": "^7.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "**/*": "npm run format",
    "**/*.{js,ts}": "npm run lint:fix"
  },
  "author": "Proyecto Arima",
  "repository": "proyecto-arima/backend",
  "license": "ISC"
}
